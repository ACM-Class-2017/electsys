{
    "update_url":"http://clients2.google.com/service/update2/crx",
    "name": "Electsys++ 交大选课插件",
    "author": "laohyx",
    "version": "2.4.2",
    "icons": {
        "48": "logo_new.png",
        "128": "logo_new_128.png"
    },
    "description": "上海交通大学选课网优化工具",
    "homepage_url": "http://laohyx.com/",
    "browser_action": {
        "default_icon": {
            "48": "logo_new.png",
            "128": "logo_new_128.png"
        },
        "default_title": "Electsys++",
        "default_popup": "popup/popup.html"
    },
    "content_scripts": [
        {
            "matches": ["*://*.sjtu.edu.cn/*"],
            "js": ["content_script/jquery-3.1.0.js",
                 "content_script/jquery-ui-1.12.0.min.js",
                 "content_script/jquery-migrate-3.0.0.js",
                 "content_script/lesson_data.js",
                 "content_script/lesson.js",
                 "content_script/nhce.js",
                 "content_script/optimize.js",
                 "content_script/score.js",
                 "content_script/login_page.js",
                 "content_script/eductionList.js",
                 "content_script/core_course.js",
                 "content_script/load.js"
            ],
            "css": [
                "content_script/jquery-ui-1.12.0.structure.css"
            ],
            "run_at": "document_start",
            "all_frames": true
        }
    ],
    "permissions": [
        "*://*.sjtu.edu.cn/*",
        "notifications",
        "management"
    ],
    "minimum_chrome_version": "49",
    "manifest_version": 2,
    "web_accessible_resources": [
        "manifest.json"
    ]
}
